  background-size: 100%;
}

.hui-block {
  display: block;
  overflow: auto;
  background-color: white;
  margin-top: 30rpx;
  position: relative;
}

.hui-block-title {
  margin-top: 0;
}

.hui-block::before {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  border-top: 1rpx solid #e5e5e5;
  content: ' ';
}

.hui-block::after {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  border-bottom: 1rpx solid #e5e5e5;
  content: ' ';
}

.hui-round {
  border-radius: 10rpx;
}

/* 空白 */

.hui-space {
  margin-right: 10rpx;
}

.hui-vskip {
  margin-top: 30rpx !important;
}

/* 表单 */

form.hui-form {
  display: block;
  overflow: auto;
}

/* 限制行数 */

.hui-ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-wrap: normal;
}

/* info inline list */

.hui-info-item {
  font-size: small;
  color: #666;
  margin-right: 20rpx;
}

/* 操作工具条 */

.hui-operation {
  background-color: #f7f7fa;
  border-top: 1rpx solid #ceced1;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
}

.hui-operation-item {
  padding: 30rpx;
  text-align: center;
  flex: 1;
}

.hui-operation-vskip {
  padding-top: 120rpx;
}

/* 缩略图 */

image.hui-image {
  width: 100%;
  display: block;
}

.hui-thumbnail {
  width: 180rpx;
  height: 180rpx;
  margin-top: 10rpx;
  margin-right: 10rpx;
}

/* Markdown 输出 */

.h2_class {
  margin-top: 40rpx;
  margin-bottom: 30rpx;
}

.h3_class {
  margin-top: 30rpx;
  margin-bottom: 25rpx;
}

.h4_class {
  margin-top: 25rpx;
  margin-bottom: 20rpx;
  text-align: center;
}

.p_class {
  text-align: justify;
  margin-bottom: 20rpx;
}

.img_class {
  max-width: 100%;
}
.hui-loadmore {
  font-size: small;
  color: #666;
  text-align: center;
  padding: 30rpx 60rpx;
}
.hui-media {
  position: relative;
  display: flex;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.hui-media::after {
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: 0;
  border-bottom: 1rpx solid #e5e5e5;
  content: ' ';
}

.hui-media:last-child::after {
  display: none;
}

.hui-media-hd {
  width: 96rpx;
  height: 96rpx;
  margin-right: 20rpx;
}

.hui-media-hd image {
  width: 96rpx;
  height: 96rpx;
}

.hui-media-bd {
  flex: 1;
}

.hui-media-bd-title {
  margin-bottom: 8rpx;
}

.hui-media-bd-desc {
  font-size: small;
  color: #666;
  text-align: justify;
}
.hui-panel-hd {
  color: #555;
}

.hui-panel-bd {
  padding: 20rpx 30rpx;
  text-align: justify;
  line-height: 1.7;
}
