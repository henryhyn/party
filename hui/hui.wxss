.hui-btn {
  margin-top: 20rpx;
}

.hui-btn:first-child {
  margin-top: 0;
}

.hui-btn-large {
  border-radius: 0;
  border: none;
}

.hui-btn-large::after {
  display: none;
}

.hui-btn-area {
  margin: 20rpx 30rpx;
}
.hui-card-bd {
  padding: 20rpx 30rpx;
}
.hui-cell {
  position: relative;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
  padding-right: 30rpx;
  height: 88rpx;
}

.hui-cell::after {
  position: absolute;
  left: 30rpx;
  right: 0;
  bottom: 0;
  border-bottom: 1rpx solid #e5e5e5;
  content: ' ';
}

.hui-cell:last-child::after {
  display: none;
}

.hui-cell-bd {
  flex: 1;
}

.hui-cell-ft {
  color: #666;
  font-size: small;
  position: relative;
}

.hui-cell-access .hui-cell-ft {
  padding-right: 20rpx;
}

.hui-cell-access .hui-cell-ft::after {
  position: absolute;
  top: 50%;
  right: 4rpx;
  content: " ";
  display: inline-block;
  height: 12rpx;
  width: 12rpx;
  border-width: 4rpx 4rpx 0 0;
  border-color: #c8c8c8;
  border-style: solid;
  transform: translateY(-50%) matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
}
.hui-c-red {
  color: #f44 !important;
}

.hui-c-blue {
  color: #3283fa !important;
}

.hui-c-green {
  color: #4b0 !important;
}
.hui-header {
  padding: 30rpx 60rpx;
}

.hui-header-title {
  margin-bottom: 8rpx;
}

.hui-header-desc {
  font-size: small;
  color: #666;
  text-align: justify;
}

.hui-footer {
  padding: 30rpx 60rpx;
  text-align: center;
  color: #1aad19;
}
.hui-grids {
  display: flex;
  flex-wrap: wrap;
}

.hui-grid {
  box-sizing: border-box;
  width: 33.33333%;
  padding: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.hui-grid-icon {
  font-size: xx-large;
}

.hui-grid-label {
  font-size: small;
}
